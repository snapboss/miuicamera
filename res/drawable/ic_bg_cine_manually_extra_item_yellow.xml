<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_activated="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/main_yellow" />
            <corners android:radius="@dimen/b92" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle" android:useLevel="false">
            <corners android:radius="@dimen/b92" />
            <stroke android:width="@dimen/b93" android:color="@color/bg_cine_manually_extra_item_color" />
        </shape>
    </item>
</selector>